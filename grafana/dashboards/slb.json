{
  "title": "阿里云 SLB 监控",
  "tags": ["network", "slb", "mce"],
  "timezone": "browser",
  "schemaVersion": 41,
  "version": 1,
  "refresh": "30s",
  "uid": "aliyun-slb-dashboard",
  "editable": true,
  "panels": [
    {
      "type": "timeseries",
      "title": "入方向带宽",
      "id": 1,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 0, "y": 0},
      "fieldConfig": {"defaults": {"unit": "bps"}, "overrides": []},
      "targets": [
        {
          "expr": "slb_traffic_rx_bps{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}",
          "legendFormat": "{{resource_id}} - 入带宽",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "出方向带宽",
      "id": 2,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 8, "y": 0},
      "fieldConfig": {"defaults": {"unit": "bps"}, "overrides": []},
      "targets": [
        {
          "expr": "slb_traffic_tx_bps{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}",
          "legendFormat": "{{resource_id}} - 出带宽",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "入方向包速率",
      "id": 3,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 0, "y": 7},
      "fieldConfig": {"defaults": {"unit": "pps"}, "overrides": []},
      "targets": [
        {
          "expr": "slb_packet_rx{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}",
          "legendFormat": "{{resource_id}} - 入包",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "出方向包速率",
      "id": 4,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 8, "y": 7},
      "fieldConfig": {"defaults": {"unit": "pps"}, "overrides": []},
      "targets": [
        {
          "expr": "slb_packet_tx{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}",
          "legendFormat": "{{resource_id}} - 出包",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "七层 5xx 状态码",
      "id": 5,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 0, "y": 14},
      "fieldConfig": {"defaults": {"unit": "ops"}, "overrides": []},
      "targets": [
        {
          "expr": "slb_status_code_5xx{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}",
          "legendFormat": "{{resource_id}} - 5xx",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "七层 QPS",
      "id": 6,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 8, "y": 14},
      "fieldConfig": {"defaults": {"unit": "ops"}, "overrides": []},
      "targets": [
        {
          "expr": "slb_qps{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}",
          "legendFormat": "{{resource_id}} - QPS",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "七层 RT",
      "id": 7,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 0, "y": 21},
      "fieldConfig": {"defaults": {"unit": "ms"}, "overrides": []},
      "targets": [
        {
          "expr": "slb_rt{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}",
          "legendFormat": "{{resource_id}} - RT",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "后端异常实例数",
      "id": 8,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 8, "y": 21},
      "fieldConfig": {"defaults": {"unit": "short"}, "overrides": []},
      "targets": [
        {
          "expr": "slb_unhealthy_server_count{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}",
          "legendFormat": "{{resource_id}} - 异常服务器",
          "refId": "A"
        }
      ]
    }
    ,
    {
      "type": "timeseries",
      "title": "实例 QPS",
      "id": 9,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 0, "y": 28},
      "fieldConfig": {"defaults": {"unit": "ops"}, "overrides": []},
      "targets": [
        {
          "expr": "slb_instance_qps{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}",
          "legendFormat": "{{resource_id}} - 实例QPS",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "实例 RT",
      "id": 10,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 8, "y": 28},
      "fieldConfig": {"defaults": {"unit": "ms", "thresholds": {"mode": "absolute", "steps": [{"color": "green"}, {"color": "red", "value": 500}] } }, "overrides": []},
      "targets": [
        {
          "expr": "slb_instance_rt{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}",
          "legendFormat": "{{resource_id}} - 实例RT",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "实例入包速率",
      "id": 11,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 0, "y": 35},
      "fieldConfig": {"defaults": {"unit": "pps"}, "overrides": []},
      "targets": [
        {
          "expr": "slb_instance_packet_rx{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}",
          "legendFormat": "{{resource_id}} - 入包",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "实例出包速率",
      "id": 12,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 8, "y": 35},
      "fieldConfig": {"defaults": {"unit": "pps"}, "overrides": []},
      "targets": [
        {
          "expr": "slb_instance_packet_tx{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}",
          "legendFormat": "{{resource_id}} - 出包",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "状态码分布",
      "id": 13,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 0, "y": 42},
      "fieldConfig": {"defaults": {"unit": "ops", "thresholds": {"mode": "absolute", "steps": [{"color": "green"}, {"color": "red", "value": 10}] } }, "overrides": []},
      "targets": [
        {"expr": "slb_status_code_2xx{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}", "legendFormat": "{{resource_id}} - 2xx", "refId": "A"},
        {"expr": "slb_status_code_3xx{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}", "legendFormat": "{{resource_id}} - 3xx", "refId": "B"},
        {"expr": "slb_status_code_4xx{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}", "legendFormat": "{{resource_id}} - 4xx", "refId": "C"},
        {"expr": "slb_status_code_5xx{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}", "legendFormat": "{{resource_id}} - 5xx", "refId": "D"}
      ]
    },
    {
      "type": "timeseries",
      "title": "实例利用率",
      "id": 14,
      "pluginVersion": "11.6.1",
      "gridPos": {"h": 7, "w": 8, "x": 8, "y": 42},
      "fieldConfig": {"defaults": {"unit": "percent", "thresholds": {"mode": "absolute", "steps": [{"color": "green"}, {"color": "red", "value": 80}] } }, "overrides": []},
      "targets": [
        {"expr": "slb_instance_qps_utilization_pct{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}", "legendFormat": "{{resource_id}} - QPS使用率", "refId": "A"},
        {"expr": "slb_instance_traffic_rx_utilization_pct{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}", "legendFormat": "{{resource_id}} - 入带宽使用率", "refId": "B"},
        {"expr": "slb_instance_traffic_tx_utilization_pct{cloud_provider=~\"$cloud_provider\", account_id=~\"$account_id\", region=~\"$region\", resource_id=~\"$resource_id\", code_name=~\"$code_name\"}", "legendFormat": "{{resource_id}} - 出带宽使用率", "refId": "C"}
      ]
    }
  ],
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "datasource",
        "label": "数据源",
        "query": "prometheus",
        "refresh": 1,
        "current": {"text": "default", "value": "prometheus"}
      },
      {
        "type": "query",
        "name": "cloud_provider",
        "label": "云厂商",
        "datasource": {"type": "prometheus", "uid": "$datasource"},
        "definition": "label_values(slb_traffic_rx_bps,cloud_provider)",
        "query": "label_values(slb_traffic_rx_bps,cloud_provider)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*"
      },
      {
        "type": "query",
        "name": "account_id",
        "label": "账号",
        "datasource": {"type": "prometheus", "uid": "$datasource"},
        "definition": "label_values(slb_traffic_rx_bps,account_id)",
        "query": "label_values(slb_traffic_rx_bps,account_id)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*"
      },
      {
        "type": "query",
        "name": "region",
        "label": "地域",
        "datasource": {"type": "prometheus", "uid": "$datasource"},
        "definition": "label_values(slb_traffic_rx_bps,region)",
        "query": "label_values(slb_traffic_rx_bps,region)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*"
      },
      {
        "type": "query",
        "name": "resource_id",
        "label": "资源ID",
        "datasource": {"type": "prometheus", "uid": "$datasource"},
        "definition": "label_values(slb_traffic_rx_bps,resource_id)",
        "query": "label_values(slb_traffic_rx_bps,resource_id)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*"
      },
      {
        "type": "query",
        "name": "code_name",
        "label": "业务名",
        "datasource": {"type": "prometheus", "uid": "$datasource"},
        "definition": "label_values(slb_traffic_rx_bps,code_name)",
        "query": "label_values(slb_traffic_rx_bps,code_name)",
        "refresh": 1,
        "includeAll": true,
        "allValue": ".*"
      }
    ]
  }
}
